{
  "name": "larsolesen.dk",
  "version": "1.0.0",
  "description": "Official site for larsolesen.dk.",
  "repository": {
    "type": "git",
    "url": "git://github.com/lsolesen/larsolesen-dk-jekyll.git"
  },
  "keywords": [
    "jekyll"
  ],
  "author": "Lars Olesen",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/lsolesen/larsolesen-dk-jekyll/issues"
  },
  "dependencies": {
  },
  "devDependencies": {
  },
  "scripts": {
    "start": "bundle exec jekyll server --port 8080 --incremental",
    "test:html": "bundle exec htmlproofer --only-4xx --assume-extension \".html\" --ignore-status-codes \"400,403,409,429\" --allow-hash-href --ignore-empty-alt --cache '{ \"timeframe\": { \"external\": \"7d\" } }' _site/",
    "clean": "bundle exec jekyll clean"
  }
}
